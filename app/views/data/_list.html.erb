<div id="fasta-record-sequence" class="col-md-8 col-sm-12 mt-3 alert alert-secondary">
	<div class="text-center">
		<h3>Select File to View/Download&nbsp;<i class="fa fa-arrow-right"></i></h3>
	</div>
</div>

<div class="col-md-4 col-sm-12 mt-3">
	<div class="list-group">
		<% fasta_records.each do |fasta_record| %>
			<%= link_to '#', class: 'list-group-item list-group-item-action fasta-record', data: {fasta_id: fasta_record.id} do %>
				<h6><%= fasta_record.filename %></h6>
				<div class="row">
					<div class="col-6">lat: <%= '%.10f' % fasta_record.latitude %></div>
					<div class="col-6">lng: <%= '%.10f' % fasta_record.longitude %></div>
				</div>
			<% end %>
		<% end %>
	</div>
</div>